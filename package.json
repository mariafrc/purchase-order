{
  "name": "app",
  "version": "0.0.0",
  "scripts": {
    "build": "tsc --p tsconfig.main.json && ng build",
    "main:watch": "wait-on http-get://localhost:4200/ && tsc --p tsconfig.main.json --watch",
    "electron:serve": "electron . --serve",
    "electron:linux": "npm run build:prod && npx electron-builder build --linux",
    "electron:windows": "npm run build:prod && npx electron-builder build --windows",
    "electron:mac": "npm run build:prod && npx electron-builder build --mac",
    "start": "npm-run-all -p ng:serve main:watch electron:serve",
    "ng:serve": "ng serve",
    "postinstall": "npx electron-builder install-app-deps",
    "lint:main": "eslint \"main/**/*.ts\" --fix",
    "lint:renderer": "eslint \"renderer/**/*.ts\" --fix"
  },
  "main": "dist/main/index.js",
  "private": true,
  "dependencies": {
    "@angular-builders/custom-webpack": "^10.0.1",
    "@angular/animations": "~10.0.4",
    "@angular/cdk": "^10.1.0",
    "@angular/common": "~10.0.4",
    "@angular/compiler": "~10.0.4",
    "@angular/core": "~10.0.4",
    "@angular/forms": "~10.0.4",
    "@angular/platform-browser": "~10.0.4",
    "@angular/platform-browser-dynamic": "~10.0.4",
    "@angular/router": "~10.0.4",
    "@nano-sql/adapter-mysql": "^2.0.4",
    "@nano-sql/core": "^2.3.7",
    "@ngrx/effects": "^9.2.0",
    "@ngrx/entity": "^9.2.0",
    "@ngrx/store": "^9.2.0",
    "@types/electron": "^1.6.10",
    "angular-cli-builders": "^2.1.2",
    "copy-webpack-plugin": "^6.1.1",
    "electron": "^10.1.2",
    "electron-builder": "^22.8.1",
    "electron-reload": "^1.5.0",
    "fs": "0.0.1-security",
    "hammerjs": "^2.0.8",
    "jwt-js-decode": "^1.4.1",
    "moment": "^2.27.0",
    "mysql": "^2.18.1",
    "npm-run-all": "^4.1.5",
    "primeflex": "^2.0.0",
    "primeicons": "^4.0.0",
    "primeng": "^10.0.0-rc.3",
    "rxjs": "~6.5.5",
    "stream": "0.0.2",
    "tslib": "^2.0.0",
    "typeorm": "^0.2.26",
    "wait-on": "^5.2.0",
    "webpack": "^4.44.2",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1000.3",
    "@angular/cli": "~10.0.3",
    "@angular/compiler-cli": "~10.0.4",
    "@ngrx/schematics": "^9.2.0",
    "@ngrx/store-devtools": "^9.2.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "codelyzer": "^6.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
